@if ( ViewBag.PageNumber != 1 )
{
    @Html.ActionLink( "|<", "Page" )
    @Html.ActionLink( "<", "Page", new { pageNumber = ViewBag.PageNumber - 1 } )
}
Page @ViewBag.PageNumber of @ViewBag.TotalPages
@if ( ViewBag.PageNumber < @ViewBag.TotalPages )
{
    @Html.ActionLink( ">", "Page", new { pageNumber = ViewBag.PageNumber + 1 } )
    <text>&nbsp;</text>
    @Html.ActionLink( ">|", "Page", new { pageNumber = ViewBag.TotalPages } )
}